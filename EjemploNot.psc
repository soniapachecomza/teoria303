Algoritmo EjemploNot
	//Variables
	Definir estado Como Logico
	Definir resultado Como Logico
		
		// Datos: Inicializamos el estado
		estado <- Verdadero
		
		// Impresión: Mostramos el estado inicial
		Escribir "Estado inicial: ", estado
		
		// Usamos NOT para invertir el estado
		resultado <- No estado
		
		// Mostramos el resultado
		Escribir "Estado después de aplicar NOT: ", resultado

FinAlgoritmo
